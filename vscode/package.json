{
  "name": "stackwise",
  "displayName": "stackwise",
  "description": "Explain what you want a function to do, and AI builds it",
  "version": "0.0.9",
  "publisher": "stackwise",
  "icon": "assets/icon.png",
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onCommand:editor.action.revealDefinition",
    "onCommand:yourExtension.someCommand",
    "onView:stackwiseContainer",
    "onTextEditorSelectionChange",
    "onTextEditorChange"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [],
    "views": {}
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/src/test/runTest.js",
    "build": "tsc"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
